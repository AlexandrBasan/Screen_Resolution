<?xml version="1.0" encoding="utf-8"?>
<FrameLayout
	xmlns:android="http://schemas.android.com/apk/res/android"
	xmlns:aw="http://schemas.android.com/apk/res/com.cbsb.screenresolution"
	xmlns:ads="http://schemas.android.com/apk/lib/com.google.ads"
	android:layout_width="fill_parent"
	android:layout_height="fill_parent"
	android:background="@drawable/whole"
	android:id="@+id/frame" xmlns:app="http://schemas.android.com/apk/res/com.cbsb.screenresolution">

	<RelativeLayout
	    android:layout_width="fill_parent"
	    android:layout_height="359dp"
	    android:gravity="top"
	    android:orientation="vertical"
	    android:paddingLeft="30dp"
	    android:paddingTop="120dp" >

		<TextView
		    android:id="@+id/sizelabel"
		    android:layout_width="wrap_content"
		    android:layout_height="wrap_content"
		    android:layout_alignRight="@+id/aspectlabel"
		    android:text="Size:"
		    android:textColor="@color/Green" />

		<TextView
			android:id="@+id/size"
			android:layout_height="wrap_content"
			android:text="@string/size"
			android:layout_alignLeft="@+id/aspect"
			android:layout_width="100dp" />

		<TextView
		    android:id="@+id/densitylabel"
		    android:layout_width="wrap_content"
		    android:layout_height="wrap_content"
		    android:layout_alignRight="@+id/aspectlabel"
		    android:layout_below="@id/sizelabel"
		    android:text="Density:"
		    android:textColor="@color/Green" />

		<TextView
			android:id="@+id/density"
			android:layout_height="wrap_content"
			android:layout_alignTop="@+id/densitylabel"
			android:layout_alignLeft="@+id/aspect"
			android:text="@string/density"
			android:layout_width="100dp" />

		<TextView
		    android:id="@+id/orientationlabel"
		    android:layout_width="wrap_content"
		    android:layout_height="wrap_content"
		    android:layout_alignRight="@+id/aspectlabel"
		    android:layout_below="@id/densitylabel"
		    android:text="Orientation:"
		    android:textColor="@color/Green" />

		<TextView
			android:id="@+id/orientation"
			android:layout_height="wrap_content"
			android:layout_alignTop="@+id/orientationlabel"
			android:layout_alignLeft="@+id/aspect"
			android:text="@string/orientation"
			android:layout_width="100dp" />

		<TextView
		    android:id="@+id/aspectlabel"
		    android:layout_width="wrap_content"
		    android:layout_height="wrap_content"
		    android:layout_below="@id/orientationlabel"
		    android:text="Aspect Ratio:"
		    android:textColor="@color/Green" />

		<TextView
			android:id="@+id/aspect"
			android:layout_height="wrap_content"
			android:layout_toRightOf="@+id/aspectlabel"
			android:layout_alignTop="@+id/aspectlabel"
			android:layout_marginLeft="10dp"
			android:text="@string/aspect"
			android:layout_width="100dp" />

		<TextView
		    android:id="@+id/TextView01"
		    android:layout_width="wrap_content"
		    android:layout_height="wrap_content"
		    android:layout_alignParentBottom="true"
		    android:layout_alignParentLeft="true"
		    android:layout_alignRight="@+id/TextView03"
		    android:text="QVGA: 320x240 (4:3)"
		    android:textColor="@color/Green" />

		<TextView
		    android:id="@+id/TextView02"
		    android:layout_width="wrap_content"
		    android:layout_height="wrap_content"
		    android:layout_above="@+id/TextView01"
		    android:layout_alignLeft="@+id/orientationlabel"
		    android:layout_alignParentLeft="true"
		    android:text="WSVGA: 1024x600 (15:9)"
		    android:textColor="@color/Green" />

		<TextView
		    android:id="@+id/TextView03"
		    android:layout_width="wrap_content"
		    android:layout_height="wrap_content"
		    android:layout_above="@+id/TextView02"
		    android:layout_alignLeft="@+id/orientationlabel"
		    android:layout_alignParentLeft="true"
		    android:text="WVGA: 800x480 (5:3)"
		    android:textColor="@color/Green" />

		<TextView
		    android:id="@+id/TextView04"
		    android:layout_width="wrap_content"
		    android:layout_height="wrap_content"
		    android:layout_above="@+id/TextView03"
		    android:layout_alignLeft="@+id/TextView03"
		    android:text="WQVGA: 400x240 (5:3)"
		    android:textColor="@color/Green" />

		<TextView
		    android:id="@+id/TextView05"
		    android:layout_width="wrap_content"
		    android:layout_height="wrap_content"
		    android:layout_above="@+id/TextView04"
		    android:layout_alignLeft="@+id/TextView04"
		    android:text="VGA: 640x480 (4:3 - 12:9)"
		    android:textColor="@color/Green" />

		<TextView
		    android:id="@+id/TextView06"
		    android:layout_width="wrap_content"
		    android:layout_height="wrap_content"
		    android:layout_above="@+id/TextView05"
		    android:layout_alignLeft="@+id/TextView05"
		    android:text="WXGA: 1280x768 (5:3)"
		    android:textColor="@color/Green" />

		<TextView
		    android:id="@+id/TextView07"
		    android:layout_width="wrap_content"
		    android:layout_height="wrap_content"
		    android:layout_above="@+id/TextView06"
		    android:layout_alignLeft="@+id/TextView06"
		    android:text="HVGA: 320x480 (2:3)"
		    android:textColor="@color/Green" />

	</RelativeLayout>
	<ImageView
		android:id="@+id/imageView1"
		android:src="@drawable/square"
		android:scaleType="fitXY"
		android:layout_height="100dp"
		android:layout_width="100dp"></ImageView>

	<TextView
	    android:id="@+id/width"
	    android:layout_width="wrap_content"
	    android:layout_height="wrap_content"
	    android:layout_gravity="bottom|center_horizontal"
	    android:textColor="@color/Green" />
	<com.cbsb.screenresolution.VerticalLabelView
	    android:id="@+id/height"
	    android:layout_width="wrap_content"
	    android:layout_height="wrap_content"
	    android:layout_gravity="right|center_vertical"
	    aw:text="TextView"
	    aw:textColor="@color/Green"
	    aw:textSize="14sp" />
	     />

</FrameLayout>